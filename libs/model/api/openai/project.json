{
  "name": "model-api-openai",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/model/api/openai/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/model/api/openai",
        "main": "libs/model/api/openai/src/index.ts",
        "tsConfig": "libs/model/api/openai/tsconfig.lib.json",
        "assets": ["libs/model/api/openai/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/model/api/openai/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/model/api/openai/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
